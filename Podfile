# platform :ios, '9.0'

target 'iTunesAPI' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iTunesAPI
  pod "iTunesSearchAPI"
  pod 'Kingfisher'
  target 'iTunesAPITests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iTunesAPIUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
